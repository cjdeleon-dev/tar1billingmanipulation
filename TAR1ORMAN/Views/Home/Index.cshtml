            <h5>Statement Of Account Report</h5>
            <p><a class="btn btn-success" href="@Url.Action("Index","SOA")">Proceed Now &raquo;</a></p>
        </div>
        <div class="col-md-4" id="tor">
            <h5>Teller's Official Receipt By Year and Month</h5>
            <p><a class="btn btn-success" href="@Url.Action("Index","TORtoPDF")">Proceed Now &raquo;</a></p>
        </div>
