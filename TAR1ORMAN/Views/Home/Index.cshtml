            <h5>Generate Disconnection List</h5>
            <p><a class="btn btn-success" href="@Url.Action("Index","DisconList")">Proceed Now &raquo;</a></p>
        </div>
        <div class="col-md-4" id="memconsupddtl">
            <h5>Member Consumer (Update Details or Change Status)</h5>
            <p><a class="btn btn-success" href="@Url.Action("MemberConsumer","MemCons")">Proceed Now &raquo;</a></p>
        </div>
        <div class="col-md-4" id="sumdtdrpt">
            <h5>Generate Summary of Disconnection Report</h5>
            <p><a class="btn btn-success" href="@Url.Action("SumDisconRpt","SumOfDisconReport")">Proceed Now &raquo;</a></p>
        </div>
        <div class="col-md-4" id="sumdtdrptlist">
