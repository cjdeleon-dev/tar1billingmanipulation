@{
    ViewBag.Title = "Home Page";
}

<style>
    * {
        font-size: 12px;
    }
</style>

<script src="~/Scripts/myscripts/homescriptv1.2.2.js"></script>

<div class="container">
    <div class="row">
        <div class="col-md-4">
            <input type="hidden" id="txtUserIdLogged" class="form-control" value=@ViewBag.UserId />
            <input type="hidden" id="txtCurrentRole" class="form-control" value=@ViewBag.Message />
        </div>
    </div>
    <div class="row">
        <div class="col-md-4" id="oradddiv">
            <h5>Changing OR by Range in Addition</h5>
            <p><a class="btn btn-success" href="@Url.Action("ORRangeInAddition","Processing")">Input OR Range Now &raquo;</a></p>
        </div>
        <div class="col-md-4" id="orsubdiv">
            <h5>Changing OR by Range in Subtraction</h5>
            <p><a class="btn btn-success" href="@Url.Action("ORRangeInSubtraction","Processing")">Input OR Range Now &raquo;</a></p>
        </div>
        <div class="col-md-4" id="orprefdiv">
            <h5>Changing Prefix of the OR Range</h5>
            <p><a class="btn btn-success" href="@Url.Action("PrefixORRange","Processing")">Input OR Range Now &raquo;</a></p>
        </div>

        <div class="col-md-4" id="lstburial">
            <h5>List of Claimed Burial Consumers</h5>
            <p><a class="btn btn-success" href="@Url.Action("Index","ClaimedBurial")">View List Now &raquo;</a></p>
