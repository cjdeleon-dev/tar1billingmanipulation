        @*<div class="col-md-4" id="disconlistv2">
            <h5>Generate Disconnection List Version 2</h5>
            <p><a class="btn btn-success" href="@Url.Action("Index","DisconListV2")">Proceed Now &raquo;</a></p>
        </div>*@
