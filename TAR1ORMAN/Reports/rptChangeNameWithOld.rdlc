      <Textbox Name="Textbox24">
        <CanGrow>true</CanGrow>
        <KeepTogether>true</KeepTogether>
        <Paragraphs>
          <Paragraph>
            <TextRuns>
              <TextRun>
                <Value>          That I, </Value>
                <Style>
                  <FontFamily>Calibri</FontFamily>
                  <FontWeight>Normal</FontWeight>
                </Style>
              </TextRun>
              <TextRun>
                <Value>=Fields!AccountName.Value</Value>
                <Style>
                  <FontFamily>Calibri</FontFamily>
                  <FontWeight>Bold</FontWeight>
                  <TextDecoration>Underline</TextDecoration>
                </Style>
              </TextRun>
              <TextRun>
                <Value>, a resident of </Value>
                <Style>
                  <FontFamily>Calibri</FontFamily>
                  <FontWeight>Normal</FontWeight>
                </Style>
              </TextRun>
              <TextRun>
                <Value>=Fields!AccountAddress.Value</Value>
                <Style>
                  <FontFamily>Calibri</FontFamily>
                  <FontWeight>Bold</FontWeight>
                  <TextDecoration>Underline</TextDecoration>
                </Style>
              </TextRun>
              <TextRun>
                <Value>, do hereby request that my Membership from the Tarlac I Electric Cooperative, Inc. (TARELCO I) be withdrawn due to </Value>
                <Style>
                  <FontFamily>Calibri</FontFamily>
                  <FontWeight>Normal</FontWeight>
                </Style>
              </TextRun>
              <TextRun>
                <Value>=Fields!Reason.Value</Value>
                <Style>
                  <FontFamily>Calibri</FontFamily>
                  <FontWeight>Bold</FontWeight>
                  <TextDecoration>Underline</TextDecoration>
                </Style>
              </TextRun>
              <TextRun>
                <Value>.</Value>
                <Style>
                  <FontFamily>Calibri</FontFamily>
                  <FontWeight>Normal</FontWeight>
                </Style>
              </TextRun>
            </TextRuns>
            <Style>
              <TextAlign>Left</TextAlign>
            </Style>
          </Paragraph>
          <Paragraph>
            <TextRuns>
              <TextRun>
                <Value />
                <Style>
                  <FontFamily>Calibri</FontFamily>
                  <FontWeight>Normal</FontWeight>
                </Style>
              </TextRun>
            </TextRuns>
            <Style>
              <TextAlign>General</TextAlign>
            </Style>
          </Paragraph>
          <Paragraph>
            <TextRuns>
              <TextRun>
                <Value>          That beacause of the above-stated purpose, I am giving my full authority to </Value>
                <Style>
                  <FontFamily>Calibri</FontFamily>
                  <FontWeight>Normal</FontWeight>
                </Style>
              </TextRun>
              <TextRun>
                <Value>=Fields!NewName.Value</Value>
                <Style>
                  <FontFamily>Calibri</FontFamily>
                  <FontWeight>Bold</FontWeight>
                  <TextDecoration>Underline</TextDecoration>
                </Style>
              </TextRun>
              <TextRun>
                <Value>, a resident of </Value>
                <Style>
                  <FontFamily>Calibri</FontFamily>
                  <FontWeight>Normal</FontWeight>
                </Style>
              </TextRun>
              <TextRun>
                <Value>=Fields!AccountAddress.Value</Value>
                <Style>
                  <FontFamily>Calibri</FontFamily>
                  <FontWeight>Bold</FontWeight>
                  <TextDecoration>Underline</TextDecoration>
                </Style>
              </TextRun>
              <TextRun>
                <Value> to use my old meter and change it to his/her name.</Value>
